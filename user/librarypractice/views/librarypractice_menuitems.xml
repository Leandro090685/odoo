<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="book_list_action" model="ir.actions.act_window">
        <field name="name"> Book List</field>
        <field name="res_model">librarypractice.book</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create the first Book
            </p>
        </field>
    </record>

    <menuitem name="Library Practice"
            id="menu_librarypractice"
            sequence="1"
            web_icon="librarypractice,static/description/icon.png">
        <menuitem id= "menu_librarypractice_sub" name="Library Practice" sequence="1">
            <menuitem id="menu_librarypractice_book" name="Books" sequence="2" action="book_list_action"/>
        </menuitem>
    </menuitem>
</odoo>